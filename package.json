    "lodash.throttle": "^4.1.0",
    "mkdirp": "^0.5.1",
    "morgan": "^1.7.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.4",
    "skin-deep": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
